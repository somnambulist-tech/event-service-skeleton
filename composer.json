{
    "name": "somnambulist/event-service-skeleton",
    "description": "An event service skeleton project that will persist any domain events",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-json": "*",
        "ext-iconv": "*",
        "adamsafr/form-request-bundle": "^0.1",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "sensiolabs/security-checker": "^6.0",
        "somnambulist/api-bundle": "~2.0",
        "somnambulist/domain": "~3.1",
        "somnambulist/read-models-bundle": "^1.1",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/messenger": "5.1.*",
        "symfony/monolog-bundle": "~3.5",
        "symfony/proxy-manager-bridge": "5.1.*",
        "symfony/property-access": "5.1.*",
        "symfony/serializer": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/doctrine-fixtures-bundle": "~3.3",
        "fzaninotto/faker": "~1.9",
        "liip/test-fixtures-bundle": "~1.9",
        "phpunit/phpunit": "~9.3",
        "symfony/browser-kit": "5.1.*",
        "symfony/css-selector": "5.1.*",
        "symfony/debug-bundle": "5.1.*",
        "symfony/flex": "^1.9",
        "symfony/phpunit-bridge": "5.1.*",
        "symfony/stopwatch": "5.1.*",
        "symfony/web-profiler-bundle": "5.1.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "process-timeout": 3600,
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "App\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Events\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
